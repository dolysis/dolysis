{
    "[Markdown]": {
        "editor.wordWrap": "wordWrapColumn",
        "editor.wordWrapColumn": 120
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "cSpell.languageSettings": [
        {
            "enabled": true,
            "languageId": [
                "rust"
            ]
        },
        {
            "enabled": true,
            "languageId": [
                "toml"
            ]
        }
    ],
    "cSpell.words": [
        "AND's",
        "Addrs",
        "Dir's",
        "Iterator's",
        "Peekable",
        "Transcodes",
        "autoclean",
        "autocmd",
        "bincache",
        "bstr",
        "cbor",
        "chmod",
        "chrono",
        "clippy",
        "compat",
        "cout",
        "dedup",
        "descrim",
        "dolysis",
        "dont",
        "echoserver",
        "endl",
        "executables",
        "fctl",
        "formframe",
        "fset",
        "intos",
        "iostream",
        "kubectl",
        "kubernetes",
        "loopframe",
        "minikube",
        "nano",
        "nanos",
        "rcxt",
        "repr",
        "serde",
        "setlocal",
        "skipframe",
        "spelllang",
        "strs",
        "subcommand",
        "symlinks",
        "thiserror",
        "unixsocket",
        "unixstream",
        "virtualbox",
        "walkdir",
        "xenial"
    ],
    "markdown.extension.print.absoluteImgPath": false,
    "markdown.extension.toc.githubCompatibility": true,
    "markdown.extension.toc.orderedList": false,
    "markdown.extension.toc.updateOnSave": true,
    "markdownlint.config": {
        "line-length": false,
        "ul-style": false,
        "code": "fenced",
    },
    "prettier.useTabs": false,
}